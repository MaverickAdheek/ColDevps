@page
@model ParkinLot.App.Frontend.Pages.EditarDuenoVehiculoModel
@{
}

<h1>Editar  sistema</h1>
<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="DuenoVehiculo.Nombres" class="col-sm-2">Nombres</label>
        <input asp-for="DuenoVehiculo.Nombres" class="from-control" >
        <span class="text-danger" asp-validation-for="DuenoVehiculo.Nombres"></span>
    </div>
    <div class="form-group row">
        <label asp-for="DuenoVehiculo.Apellidos" class="col-sm-2">Apellidos</label>
        <input asp-for="DuenoVehiculo.Apellidos" class="from-control" >
        <span class="text-danger" asp-validation-for="DuenoVehiculo.Apellidos"></span>
    </div>
    <div class="form-group row">
        <label asp-for="DuenoVehiculo.Documento" class="col-sm-2">Documento</label>
        <input asp-for="DuenoVehiculo.Documento" class="from-control"  required>
        <span class="text-danger" asp-validation-for="DuenoVehiculo.Documento"></span>
    </div>
    <div class="form-group row">
        <label asp-for="DuenoVehiculo.Usuario" class="col-sm-2">Usuario</label>
        <input asp-for="DuenoVehiculo.Usuario" class="from-control"  required>
        <span class="text-danger" asp-validation-for="DuenoVehiculo.Usuario"></span>
    </div>
    <div class="form-group row">
        <label asp-for="DuenoVehiculo.Contrasena" class="col-sm-2">Contraseña</label>
        <input asp-for="DuenoVehiculo.Contrasena" class="from-control"  required>
        <span class="text-danger" asp-validation-for="DuenoVehiculo.Contrasena"></span>
    </div>
    
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Editar</button>
        <a asp-page="./ListaDuenosVehiculos" class="btn btn-primary">Cancelar</a>
    </div>
</form>

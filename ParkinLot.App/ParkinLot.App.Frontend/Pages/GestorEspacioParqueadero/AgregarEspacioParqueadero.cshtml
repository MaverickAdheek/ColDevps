@page
@model ParkinLot.App.Frontend.Pages.AgregarEspacioParqueaderoModel
@{
}
<h1>Agregar Espacio Parqueadero</h1>
<form method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="espacioParqueadero.TipoEspacio" class="col-sm-2">Tipo de Espacio</label>
        <Select asp-for="espacioParqueadero.TipoEspacio" class="from-control"> 
            <option value="Carro">Carro</option>
            <option value="Moto">Moto</option>
        </Select>
        <span class="text-danger" asp-validation-for="espacioParqueadero.TipoEspacio"></span>
    </div>
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Guardar</button>
        <a asp-page="./ListaEspaciosParqueaderos" class="btn btn-primary">Cancelar</a>
    </div>
</form>
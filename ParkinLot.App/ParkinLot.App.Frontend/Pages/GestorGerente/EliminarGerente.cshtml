@page
@model ParkinLot.App.Frontend.Pages.EliminarGerenteModel
@{
}
<h1>Eliminar Gerente @Model.Gerente.Nombres</h1>
<form method="post" class="mt-3">
    <input hidden asp-for="Gerente.CodigoEmpleado">
    <div class="form-group row">
        <button type="submit" class="btn btn-primary">Eliminar</button>
        <a asp-page="./ListaGerentes" class="btn btn-primary">Cancelar</a>
    </div>
</form>

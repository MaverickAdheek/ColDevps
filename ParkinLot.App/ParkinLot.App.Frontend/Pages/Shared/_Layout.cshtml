<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ParkinLot</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-page="/Index">ParkinLot</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Index">Home</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorAdministradorSistema/ListaAdministradoresSistema">Administradores del sistema</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorGerente/ListaGerentes">Gestor gerentes</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorAuxiliar/ListaAuxiliares">Gestor auxiliares</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorDuenoVehiculo/ListaDuenosVehiculos">Gestor clientes</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorVehiculo/ListaVehiculos">Gestor vehículos</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorReserva/ListaReservas">Gestor reservas</a>
                        </li>
                        <li class="nav-item"> <!-- Nueva página -->
                            <a class="nav-link text-dark" asp-area="" asp-page="/GestorEspacioParqueadero/ListaEspaciosParqueadero">Gestor espacios parqueadero</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - ParkinLot.App.Frontend - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
